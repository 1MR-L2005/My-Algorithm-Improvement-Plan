class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int slow=0;
        int quick=0;
        while(true)
        {
            slow=nums[slow];
            quick=nums[nums[quick]];
            if(slow==quick)break;
        }
        quick=0;
        while(quick!=slow)
        {
            slow=nums[slow];
            quick=nums[quick];
        }
        return slow;
    }
};
