class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.size()!=t.size())return false;
        map<char,int>s_new;
        map<char,int>t_new;
        for(int i=0;i<s.size();i++)
        {
            s_new[s[i]]++;
            t_new[t[i]]++;
        }
        for(auto pr:s_new)
        {
            if(s_new[pr.first]==t_new[pr.first]){}
            else return false;
        }
        return true;
    }
};
