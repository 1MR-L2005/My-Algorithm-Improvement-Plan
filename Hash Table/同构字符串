class Solution {
public:
    bool isIsomorphic(string s, string t) {
        map<char,char>s_and_t;
        map<char,char>t_and_s;
        for(int i=0;i<s.size();i++)
        {
            if(s_and_t[s[i]]&&s_and_t[s[i]]!=t[i])return false;
            else if(t_and_s[t[i]]&&t_and_s[t[i]]!=s[i])return false;
            else {
                s_and_t[s[i]]=t[i];
                t_and_s[t[i]]=s[i];
            }
        }
        return true;
    }
};
