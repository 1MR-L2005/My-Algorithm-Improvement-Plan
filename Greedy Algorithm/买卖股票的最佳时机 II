class Solution {
public:
    int maxProfit(vector<int>& prices) {
        if(prices.size()==1)return 0;
        int curror=prices[0];
        int total=0;
        for(int i=1;i<prices.size();i++)
        {
            if(curror<prices[i]){
                total+=(prices[i]-curror);
                curror=prices[i];
            }
            else curror=prices[i];
        }
        return total;
    }
};
